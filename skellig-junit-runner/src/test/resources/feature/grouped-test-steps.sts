name('Create user .*') {
    assert {
        fromTest = t1
        all_match [
                 'dude'
                 ]
    }
}

name('Get user name (.*)'){
    id = t1

    variables {
        usr = ${1}
    }

    captureData = ${usr}
}
// check all types of parameters passed in the name and as a Map
name('User (.+) exist') {
    test = 'Check user exist'
    failed {
               test = 'Get user name ${1}'
               passed {
                          test = 'Create user ${usr}'
                      }
           }
}

name('User (.+) is signed in') {
    test = 'User ${1} exist'
}