# simple test step

name (Simple test step){
 url = ${baseUrl}/a/b/c
 method = POST
 payload {
      type = json
      json {
             command = "go"
            value = "v 1 2 3"
      }
 }
}

name( Send \d{1} message (.*) from csv \(test\)) {
 url = /a/b/c
 method = POST
 auth {
    username = ${user}
    password = ${password}
 }
 body {
    template {
        file = /resources/templates/msg_get(id).ftl
        csv
        {
          file = /resources/data/test1.csv
          row {
              name = test data with valid values
              group = 1
          }
          convert = TABLE
        }
    }}
 }